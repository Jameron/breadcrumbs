{
    "name": "jameron/breadcrumbs",
    "description": "Breadcrumb builder for Laravel and PHP applications.",
    "keywords": ["breadcrumb", "laravel", "php"],
    "homepage": "http://cjmacfarlane.com",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Cameron Macfarlane",
            "email": "cammac1984@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"Jameron\\": "src/"}
    },
    "autoload-dev": {
        "classmap": [
            "tests/BreadcrumbTest.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jameron\\Breadcrumb\\Providers\\BreadcrumbServiceProvider"
            ],
            "aliases": {
                "Breadcrumb": "Jameron\\Breadcrumb\\Facades\\Facade"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
